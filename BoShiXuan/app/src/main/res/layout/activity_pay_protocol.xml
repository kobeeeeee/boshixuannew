<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <include layout="@layout/layout_title_bar"
            android:id="@+id/include16" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/scrollView2"
            android:layout_below="@+id/include16"
            android:layout_centerHorizontal="true" >

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/regular_pay_protocol"
            android:textSize="11.5sp"
            android:lineSpacingExtra="7.5dp"
            android:textColor="@color/lease_product_gray"
            android:layout_marginTop="11.5dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="14.5dp"
            android:id="@+id/textView82"
            android:layout_centerHorizontal="true" />
        </ScrollView>

    </RelativeLayout>
</LinearLayout>
