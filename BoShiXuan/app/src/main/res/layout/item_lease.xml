<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="194dp">

    <RelativeLayout
        android:id = "@+id/leaseLayout"
        android:layout_width="match_parent"
        android:layout_height="194dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="包袋"
            android:id="@+id/leaseTextCH"
            android:textSize="16sp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="268.5dp"
            android:layout_marginTop="77dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Bags"
            android:textSize="16sp"
            android:id="@+id/leaseTextEN"
            android:layout_marginTop="8dp"
            android:layout_below="@+id/leaseTextCH"
            android:layout_alignRight="@+id/leaseTextCH"
            android:layout_alignEnd="@+id/leaseTextCH" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView9"
            android:src="@drawable/lease_arrow"
            android:layout_centerVertical="true"
            android:layout_marginLeft="31dp"
            android:layout_toRightOf="@+id/leaseTextCH"
            android:layout_toEndOf="@+id/leaseTextCH" />
    </RelativeLayout>
</LinearLayout>
