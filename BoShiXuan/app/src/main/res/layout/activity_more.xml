<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <include layout="@layout/layout_title_bar"
            android:id="@+id/include5"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/memberLevel"
            android:layout_marginTop="17dp"
            android:src="@drawable/more_member_level"
            android:layout_below="@+id/include5"
            android:layout_alignLeft="@+id/stayTuned"
            android:layout_alignStart="@+id/stayTuned" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="会员等级"
            android:id="@+id/textView20"
            android:layout_marginTop="10dp"
            android:textSize="15sp"
            android:layout_below="@+id/memberLevel"
            android:layout_alignLeft="@+id/memberLevel"
            android:layout_alignStart="@+id/memberLevel" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/scoreMall"
            android:layout_marginLeft="60dp"
            android:src="@drawable/more_score_mall"
            android:layout_alignTop="@+id/memberLevel"
            android:layout_toRightOf="@+id/memberLevel"
            android:layout_toEndOf="@+id/memberLevel" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="积分商城"
            android:id="@+id/textView21"
            android:layout_marginTop="10dp"
            android:textSize="15sp"
            android:layout_below="@+id/scoreMall"
            android:layout_alignLeft="@+id/scoreMall"
            android:layout_alignStart="@+id/scoreMall" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/cloudPurchase"
            android:layout_marginLeft="60dp"
            android:src="@drawable/more_cloud_purchase"
            android:layout_alignTop="@+id/scoreMall"
            android:layout_toRightOf="@+id/scoreMall"
            android:layout_toEndOf="@+id/scoreMall" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1元云购"
            android:id="@+id/textView22"
            android:layout_marginTop="10dp"
            android:textSize="15sp"
            android:layout_below="@+id/cloudPurchase"
            android:layout_alignLeft="@+id/cloudPurchase"
            android:layout_alignStart="@+id/cloudPurchase" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/stayTuned"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="27.5dp"
            android:src="@drawable/more_stay_tuned"
            android:layout_below="@+id/textView20"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="敬请期待"
            android:id="@+id/textView23"
            android:textSize="15sp"
            android:layout_marginTop="10dp"
            android:layout_below="@+id/stayTuned"
            android:layout_alignLeft="@+id/stayTuned"
            android:layout_alignStart="@+id/stayTuned" />
    </RelativeLayout>
</LinearLayout>
